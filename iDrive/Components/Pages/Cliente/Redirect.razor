@page "/Cliente"
@inject NavigationManager Navigation;
<h3>Redirect</h3>

@code {
	bool isLoggedIn = false;
	
	protected override void OnInitialized()
	{
		string paginaDestino = "/Cliente/Login";

		if (isLoggedIn)
		{
			paginaDestino = "/Cliente/Home";
		}

		Navigation.NavigateTo(paginaDestino);
	}

}
